function backup_create()

StatusDlg.Show(MB_ICONNONE, false);
StatusDlg.SetTitle("WinDS PRO Backup...");

backup_name = _SourceFolder.."\\backup\\backup-" .. System.GetDate(DATE_FMT_ISO) .. '-' .. String.Replace(System.GetTime(TIME_FMT_MIL), ":", "-", false)

Folder.Create(backup_name)
Folder.Create(backup_name.."\\no$gba\\")
Folder.Create(backup_name.."\\no$gba_\\")
Folder.Create(backup_name.."\\desmume\\")
Folder.Create(backup_name.."\\vbalink\\")
Folder.Create(backup_name.."\\vbam\\")

File.Copy("emu\\no$gba\\BATTERY\\*.*", backup_name.."\\no$gba\\", true, false, false, false)
File.Copy("emu\\no$gba_\\BATTERY\\*.*", backup_name.."\\no$gba_\\", true, false, false, false)
File.Copy("emu\\desmume\\archivo\\*.*", backup_name.."\\desmume\\", true, false, false, false)
File.Copy("emu\\vbalink\\archivo\\*.*", backup_name.."\\vbalink\\", true, false, false, false)
File.Copy("emu\\vbam\\archivo\\*.*", backup_name.."\\vbam\\", true, false, false, false)

StatusDlg.Hide();

return backup_name;
end

