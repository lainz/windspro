<Event>
<Name>On Show</Name>
<Args/>
<Script>reiniciar = false

Label.SetText("op",version_h1);

if ComboBox.GetCount("gameboy") ~= 3 then ComboBox.AddItem("gameboy", siempre_preguntar, "preguntar") end
if ComboBox.GetCount("gameboycolor") ~= 3 then ComboBox.AddItem("gameboycolor", siempre_preguntar, "preguntar") end
if ComboBox.GetCount("gameboyadvance") ~= 5 then ComboBox.AddItem("gameboyadvance", siempre_preguntar, "preguntar") end
if ComboBox.GetCount("nintendods") ~= 10 then ComboBox.AddItem("nintendods", siempre_preguntar, "preguntar") end

ComboBox.SetSelected("gameboy", ComboBox.FindItem("gameboy", -1, LB_BYDATA, emu_game_boy))
ComboBox.SetSelected("gameboycolor", ComboBox.FindItem("gameboycolor", -1, LB_BYDATA, emu_game_boy_color))
ComboBox.SetSelected("gameboyadvance", ComboBox.FindItem("gameboyadvance", -1, LB_BYDATA, emu_game_boy_advance))
ComboBox.SetSelected("nintendods", ComboBox.FindItem("nintendods", -1, LB_BYDATA, emu_nintendo_ds))
ComboBox.SetSelected("nocashpmp", ComboBox.FindItem("nocashpmp", -1, LB_BYDATA, mejora_nocashpmp))
ComboBox.SetSelected("nocashwtt", ComboBox.FindItem("nocashwtt", -1, LB_BYDATA, mejora_nocashwtt))

-- backup
if auto_bkp == "si" then
  auto_bkp = true
else
  auto_bkp = false
end

CheckBox.SetChecked("auto_bkp_op", auto_bkp);

-- pmp
if mejora_nocashpmp == "si" then
  mejora_nocashpmp = true
else
  mejora_nocashpmp = false
end

CheckBox.SetChecked("pmp_op", mejora_nocashpmp);

-- wtt
if mejora_nocashwtt == "si" then
  mejora_nocashwtt = true
else
  mejora_nocashwtt = false
end

CheckBox.SetChecked("wtt_op", mejora_nocashwtt);

if busco_skins == nil then
local nombre_skins_instalados = Folder.Find(_SourceFolder.."\\windspro\\skins", "*", false)
if nombre_skins_instalados ~= nil then
	for indice in pairs(nombre_skins_instalados) do
		local ruta_del_skin = String.SplitPath(nombre_skins_instalados[indice])
		ComboBox.AddItem("skin", ruta_del_skin.Filename, ruta_del_skin.Filename)
	end
	ComboBox.SetSelected("skin", ComboBox.FindItem("skin", -1, LB_BYTEXT, skin))
end
busco_skins = "no"
end</Script>
