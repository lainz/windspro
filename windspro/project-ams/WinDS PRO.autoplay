function abrir()
	local dialogo_archivo = Dialog.FileBrowse(true, "", "", "WinDS PRO|*.gb;*.gmb;*.sgb;*.cgb;*.gbc;*.agb;*.gba;*.elf;*.mb;*.bin;*.nds;*.dsi;*.srl;*.pme;*.ds.gba;*.sc.nds;*3ds;*.cci;*.csu;*.rar;*.zip;*.7z;*.xz;*.windsproskin;*.nozskin|Game Boy|*.gb;*.gmb;*.sgb|Game Boy Color|*.cgb;*.gbc|Game Boy Advance|*.agb;*.gba;*.elf;*.mb;*.bin|Nintendo DS|*.nds;*.dsi;*.srl;*.pme;*.ds.gba;*.sc.nds|Nintendo 3DS|*.3ds;*.cci;*.csu|", "", "", false, true)
		if dialogo_archivo[1] ~= "CANCEL" and dialogo_archivo[1] ~= nil then
			archivo = String.SplitPath(dialogo_archivo[1])
			archivo_ruta = "\""..archivo.Drive..archivo.Folder..archivo.Filename..archivo.Extension.."\""
			extension()
		end
end

function extension()
	if String.CompareNoCase(String.Right(archivo.Filename, 3), ".ds") == 0 and String.CompareNoCase(archivo.Extension, ".gba") == 0 then abrir_nintendo_ds()
	elseif String.CompareNoCase(String.Right(archivo.Filename, 3), ".sc") == 0 and String.CompareNoCase(archivo.Extension, ".nds") == 0 then abrir_nintendo_ds()
	elseif String.CompareNoCase(archivo.Extension, ".gb") == 0 then abrir_game_boy()
	elseif String.CompareNoCase(archivo.Extension, ".gmb") == 0 then abrir_game_boy()
	elseif String.CompareNoCase(archivo.Extension, ".sgb") == 0 then abrir_game_boy()
	elseif String.CompareNoCase(archivo.Extension, ".cgb") == 0 then abrir_game_boy_color()
	elseif String.CompareNoCase(archivo.Extension, ".gbc") == 0 then abrir_game_boy_color()
	elseif String.CompareNoCase(archivo.Extension, ".agb") == 0 then abrir_game_boy_advance()
	elseif String.CompareNoCase(archivo.Extension, ".gba") == 0 then abrir_game_boy_advance()
	elseif String.CompareNoCase(archivo.Extension, ".elf") == 0 then abrir_game_boy_advance()
	elseif String.CompareNoCase(archivo.Extension, ".mb") == 0 then abrir_game_boy_advance()
	elseif String.CompareNoCase(archivo.Extension, ".bin") == 0 then abrir_game_boy_advance()
	elseif String.CompareNoCase(archivo.Extension, ".nds") == 0 then abrir_nintendo_ds()
	elseif String.CompareNoCase(archivo.Extension, ".dsi") == 0 then abrir_nintendo_ds()
	elseif String.CompareNoCase(archivo.Extension, ".srl") == 0 then abrir_nintendo_ds()
	elseif String.CompareNoCase(archivo.Extension, ".pme") == 0 then abrir_nintendo_ds()
	elseif String.CompareNoCase(archivo.Extension, ".3ds") == 0 then abrir_nintendo_3ds()
	elseif String.CompareNoCase(archivo.Extension, ".3dsx") == 0 then abrir_nintendo_3ds()
	elseif String.CompareNoCase(archivo.Extension, ".cci") == 0 then abrir_nintendo_3ds()
	elseif String.CompareNoCase(archivo.Extension, ".cxi") == 0 then abrir_nintendo_3ds()
	elseif String.CompareNoCase(archivo.Extension, ".csu") == 0 then abrir_nintendo_3ds()
	elseif String.CompareNoCase(archivo.Extension, ".rar") == 0 then extraer()
	elseif String.CompareNoCase(archivo.Extension, ".zip") == 0 then extraer()
	elseif String.CompareNoCase(archivo.Extension, ".7z") == 0 then extraer()
	elseif String.CompareNoCase(archivo.Extension, ".xz") == 0 then extraer()
	elseif String.CompareNoCase(archivo.Extension, ".windsproskin") == 0 then abrir_windspro("windsproskin")
	elseif String.CompareNoCase(archivo.Extension, ".nozskin") == 0 then abrir_windspro("nozskin")
	else informacion()
	end
end

function extraer()
	Folder.Create(_TempFolder.."\\windspro.tmp\\"..archivo.Filename)
	local retorno = File.Run(_SourceFolder.."\\windspro\\plugins\\7z.exe", "x "..archivo_ruta.." -y -aoa -r -i!*.gb -i!*.gmb -i!*.sgb -i!*.cgb -i!*.gbc -i!*.agb -i!*.gba -i!*.elf -i!*.mb -i!*.bin -i!*.nds -i!*.dsi -i!*.srl -i!*.pme -i!*.ds.gba -i!*.sc.nds -i!*.3ds -i!*.rar -i!*.zip -i!*.7z -i!*.xz", _TempFolder.."\\windspro.tmp\\"..archivo.Filename, SW_HIDE, true)
		if retorno == 0 then
			local encontrado = File.Find(_TempFolder.."\\windspro.tmp\\"..archivo.Filename, "*.*", true, false, nil, nil)
			if encontrado ~= nil then
				for indice in pairs(encontrado) do
					File.DeleteOnReboot(encontrado[indice])
				end
				if Table.Count(encontrado) ~= 1 then
					local tabla_nombre = {}
					for indice in pairs(encontrado) do
						local ruta = String.SplitPath(encontrado[indice])
						Table.Insert(tabla_nombre, indice, ruta.Filename..ruta.Extension)
					end
					local seleccionado = Dialog.ComboBox(dialogo_abrir, dialogo_seleccionar, tabla_nombre, tabla_nombre[1], false, false, MB_ICONNONE)
					for indice in pairs(tabla_nombre) do
						if seleccionado == tabla_nombre[indice] then
							archivo = String.SplitPath(encontrado[indice])
							archivo_ruta = "\""..archivo.Drive..archivo.Folder..archivo.Filename..archivo.Extension.."\""
							extension()
							break
						end
					end
				else
					archivo = String.SplitPath(encontrado[1])
					archivo_ruta = "\""..archivo.Drive..archivo.Folder..archivo.Filename..archivo.Extension.."\""
					extension()
				end
			end
		end
end

