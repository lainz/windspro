--mejora_nocashpmp = 
--mejora_nocashwtt = 

-- backup
auto_bkp = CheckBox.GetChecked("auto_bkp_op");

if auto_bkp then
  auto_bkp = "si"
else
  auto_bkp = "no"
end

-- pmp
mejora_nocashpmp = CheckBox.GetChecked("pmp_op");

if mejora_nocashpmp then
  mejora_nocashpmp = "si"
else
  mejora_nocashpmp = "no"
end

-- wtt
mejora_nocashwtt = CheckBox.GetChecked("wtt_op");

if mejora_nocashwtt then
  mejora_nocashwtt = "si"
else
  mejora_nocashwtt = "no"
end

if ComboBox.GetItemData("skin", ComboBox.GetSelected("skin")) ~= skin then
	reiniciar = true
end

if reiniciar then
	picture = "predeterminado"
	skin = ComboBox.GetItemData("skin", ComboBox.GetSelected("skin"))
	File.Run("windspro.exe", "", "", SW_SHOWNORMAL, false)
	Application.Exit(0)
end</Script>
