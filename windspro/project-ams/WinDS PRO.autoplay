<Script>emu_game_boy = ComboBox.GetItemData("gameboy", ComboBox.GetSelected("gameboy"))
emu_game_boy_color = ComboBox.GetItemData("gameboycolor", ComboBox.GetSelected("gameboycolor"))
emu_game_boy_advance = ComboBox.GetItemData("gameboyadvance", ComboBox.GetSelected("gameboyadvance"))
